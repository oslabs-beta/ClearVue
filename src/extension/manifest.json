{
    "name": "Vue Toolkit",
    "version": "1.0",
    "manifest_version": 3,
    "minimum_chrome_version": "10.0",
    "devtools_page": "devtools.html",
    "permissions": [
        "tabs"
    ],
    "background": {
      "service_worker": "background-wrapper.js"
    },
    "content_scripts": [
        {
          "matches": ["<all_urls>"],
          "js": ["content-script.js"]
        }
      ]
  }