{
    "name": "Vue Toolkit",
    "version": "1.0",
    "manifest_version": 3,
    "minimum_chrome_version": "10.0",
    "devtools_page": "devtools.html",
    "permissions": [
        "tabs",
        "activeTab",
        "scripting"
    ],
    "host_permissions": [
        "<all_urls>"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "web_accessible_resources": [{
        "resources": ["backend/script.js"],
        "matches": ["<all_urls>"]
      }]
  }